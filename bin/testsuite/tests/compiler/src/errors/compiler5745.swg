//#global testerror "Err0017"
var G: string

//#global skip
interface Printable
{
    mtd const printDetails();
}

struct Book
{
    title: string = "TOTO"
}

impl Printable for Book
{
    mtd const impl printDetails()
    {
        G = me.title
    }
}

struct Magazine
{
    issueNumber: s32
}

impl Printable for Magazine
{
    mtd const impl printDetails()
    {
        @print("Magazine Issue: ", me.issueNumber)
    }
}

