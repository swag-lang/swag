#global testerror
func(T) add(x, y: T)->T
    #validif
    {
        if cast(bool) (@typeof(T).flags & .Integer)
            return true
        @compilererror("type " ++ @typeof(T).name ++ " is not supported", @location(x))
        return false
    }
{
    return x + y
}

#test
{
    v := add("1", "2")
}