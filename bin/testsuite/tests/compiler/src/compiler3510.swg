//#global skip
//#[Swag.PrintBc]
#test
{
    #[Swag.Inline]
    func getInc()->closure()->s32
    {
        x := 10
        return closure|x|()->s32 {
            x += 1
            return x
        }
    }

    fct := getInc()
    @assert(fct() == 11)
    @assert(fct() == 12)
    @assert(fct() == 13)
}