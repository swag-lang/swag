
using Core

#test
{
    var arr: Array'String
    arr.addAny("0")
    arr.addAny("1")
    arr.addAny("2")
    arr.addAny("3")
    Slice.insertionSort(@mkslice(arr.buffer, cast(u64) arr.count), func(x, y) => y <=> x)
    @assert(arr[0] == "3")
    @assert(arr[1] == "2")
    @assert(arr[2] == "1")
    @assert(arr[3] == "0")
    //visit &it: arr
    //    Console.write("%", :it)
}