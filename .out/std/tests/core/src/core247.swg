using core, hash

#test
{
    {
        digest := Md5.compute("")
        const r = @[0xd4, 0x1d, 0x8c, 0xd9, 0x8f, 0x00, 0xb2, 0x04, 0xe9, 0x80, 0x09, 0x98, 0xec, 0xf8, 0x42, 0x7e]
        visit c, i: digest
            @assert(c == r[i])
    }

    {
        digest := Md5.compute("toto la foule")
        const r = @[0xd9, 0x9a, 0xd5, 0x83, 0x4a, 0xda, 0xf4, 0x24, 0xeb, 0xe9, 0x87, 0x7b, 0xe3, 0xb8, 0x49, 0x97]
        visit c, i: digest
            @assert(c == r[i])
    }
}